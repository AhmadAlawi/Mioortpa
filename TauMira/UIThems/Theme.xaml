                                            <ContextMenu x:Name="cmTab" Visibility="Hidden"  >
                                                <MenuItem x:Name="WorkSpaceMenuItem" Header="Hide/Show WorkSpaces Panel" Click="Hid_WorkSpacePael_Event"/>
                                                <MenuItem x:Name="ActionBarMenuItem" Header="Hide/Show Action Bar" Click="Hid_ActionBar_Event"/>
                                                <MenuItem x:Name="GroupPanelMenuItem" Header="Hide/Show Groups Panel" Click="Hid_GroupPanel_Event"/>
                                            </ContextMenu>
                                        </Button.ContextMenu>
                                    </Button>
